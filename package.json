{
  "name": "nft-scam-detector",
  "version": "1.0.6",
  "description": "A lightweight detector scam classifier for NFTs with 95% accuracy. Can run anywhere that webassembly runs: on a server, in a lambda function, and even running entirely in your browser.",
  "main": "index.js",
  "scripts": {
    "dev": "run-p build:watch start:watch",
    "train": "yarn build && yarn start:train",
    "eval": "yarn build && dotenv -- node dist/test/eval.js",
    "test": "yarn build && dotenv -- node dist/test.js",
    "build": "swc src -d dist --copy-files",
    "build:watch": "swc src -d dist --copy-files --watch",
    "start": "dotenv -- node dist/index.js",
    "start:watch": "dotenv -- nodemon dist/index.js",
    "start:train": "dotenv -- node dist/train.js"
  },
  "author": "toanbk21096@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "@mysten/sui.js": "^0.49.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.12",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nft-scam-detector": "^1.0.5",
    "nodemon": "^3.0.3",
    "numeral": "^2.0.6",
    "tesseract.js": "^5.1.0",
    "uuid": "^9.0.1",
    "viem": "^2.7.1"
  },
  "devDependencies": {
    "@types/numeral": "^2.0.5",
    "@types/uuid": "^9.0.8",
    "dotenv-cli": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "swc": "^1.0.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
